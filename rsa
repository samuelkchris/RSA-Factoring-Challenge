#!/usr/bin/env bash

# Function to check if a number is a factor of another number
check_factor() {
  # Check if three arguments were provided
  if [ $# -ne 3 ]; then
    # If not, multiply all arguments together
    num2=1
    for a in "$@"; do
      num2=$((num2 * a))
    done
  else
    num2=$3
  fi

  num1=$2

  # Swap num1 and num2 if num2 is greater than num1
  if ((num2 > num1)); then
    numcp=$num1
    num1=$num2
    num2=$numcp
  fi

  # Print the result in the required format
  echo "$num1*$num2"
}

# Check if a file was provided as an argument
if [ $# -ne 1 ]; then
  echo 'Usage: rsa <file>'
  exit 1
else
  # Read each line of the file and check if the numbers are factors of each other
  while read line; do
    result=$(check_factor $line)
    echo "$result"
  done < "$1"
fi
